{
  "independent": false,
  "packages": [
    "packages/*"
  ],
  "command": {
    "exec": {
      "stream": true
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "publish": {
      "forcePublish": "*",
      "allowBranch": "packages-update"
    }
  },
  "version": "0.5.15"
}
